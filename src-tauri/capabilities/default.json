{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default", 
    "dialog:default",
    "pty:allow-spawn",
    "pty:allow-exitstatus",
    "pty:allow-read",
    "pty:allow-resize",
    "pty:allow-write",
    "pty:allow-kill"
  ]
}